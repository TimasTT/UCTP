<!DOCTYPE html>
<%#include "objects/auditory.h" %>
<html>
<head>
  <meta charset="UTF-8">
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>

<h1>Listing Auditory</h1>

<%== linkTo("Create a new Auditory", urla("create")) %><br />
<br />
<table border="1" cellpadding="5" style="border: 1px #d0d0d0 solid; border-collapse: collapse;">
  <tr>
    <th>ID</th>
    <th>AuditoryName</th>
    <th>AuditoryType</th>
  </tr>
<% tfetch(QList<Auditory>, auditoryList); %>
<% for (const auto &i : auditoryList) { %>
  <tr>
    <td><%= i.id() %></td>
    <td><%= i.auditoryName() %></td>
    <td><%= i.auditoryType() %></td>
    <td>
      <%== linkTo("Show", urla("show", i.id())) %>
      <%== linkTo("Edit", urla("save", i.id())) %>
      <%== linkTo("Remove", urla("remove", i.id()), Tf::Post, "confirm('Are you sure?')") %>
    </td>
  </tr>
<% } %>
</table>

</body>
</html>
